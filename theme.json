{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"color": {
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#1a4567"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#e67e22"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"color": "#222222"
				},
				{
					"name": "Light",
					"slug": "light",
					"color": "#f5f5f5"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"name": "Primary",
					"slug": "primary",
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"fontFamily": "Georgia, serif"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--light)",
			"text": "var(--wp--preset--color--dark)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}